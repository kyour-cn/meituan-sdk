{
    "name": "kyour-cn/meituan-sdk",
    "description": "美团开放平台PHP SDK",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": ">=6.5"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.6"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Kyour",
            "email": "kyour@outlook.com"
        },
        {
            "name": "Abbotton",
            "email": "uctoo@foxmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "MeiTuan\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MeiTuan\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    }
}
